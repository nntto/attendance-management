{
  "name": "front",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint --dir src",
    "lint:fix": "yarn lint --fix",
    "format": "prettier --write --ignore-path .gitignore './**/*.{js,jsx,ts,tsx,json}'",
    "generate-typescript-axios": "openapi-generator-cli generate -g typescript-axios -i ./api_schema/reference/attendance-management-api.yaml -o ./types/typescript-axios",
    "generate-document": "npx openapi-generator-cli generate -g openapi-yaml -i ./api_schema/reference/attendance-management-api.yaml -o ./api_schema/dist --skip-operation-example",
    "start-mock": "prism mock ./api_schema/dist/openapi/openapi.yaml"
  },
  "dependencies": {
    "@prisma/client": "^4.4.0",
    "next": "12.3.1",
    "react": "18.2.0",
    "react-dom": "18.2.0"
  },
  "devDependencies": {
    "@openapitools/openapi-generator-cli": "^2.5.2",
    "@stoplight/prism-cli": "^4.10.5",
    "@types/node": "18.8.3",
    "@types/react": "18.0.21",
    "@types/react-dom": "18.0.6",
    "eslint": "8.24.0",
    "eslint-config-next": "12.3.1",
    "eslint-config-prettier": "^8.5.0",
    "eslint-import-resolver-typescript": "^3.5.2",
    "prettier": "^2.7.1",
    "prisma": "^4.4.0",
    "typescript": "4.8.4"
  }
}
